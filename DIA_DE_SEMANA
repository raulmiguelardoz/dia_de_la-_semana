import calendar

def day_of_week(dia,mes,año):
    dias_semana={0:'lunes',1:'martes',2:'miercoles',3:'jueves',4:'viernes',5:'sabado',6:'domingo'}
    d_s=calendar.weekday(año,mes,dia)
    if d_s in dias_semana:
        print(dias_semana[d_s])
    
day_of_week(17,8,1993)
day_of_week(1,1,2000)
day_of_week(18,11,1995)
day_of_week(31,12,2019)
day_of_week(20,5,1992)

RESPUESTA

MARTES
SABADO
MARTES
